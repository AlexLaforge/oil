{
  "name": "oil",
  "version": "1.0.7",
  "description": "Module for Axel Springer Oil",
  "main": "http_server.js",
  "scripts": {
    "start": "npm install && node http_server.js",
    "watch": "cross-env NODE_ENV=development webpack-dev-server --hot --config etc/webpack.config.js --inline --progress --port 3000 ",
    "prebuild": "npm run check && rm -rf dist/*",
    "build": "cross-env NODE_ENV=debug webpack --config etc/webpack.config.js && cross-env NODE_ENV=production webpack --config etc/webpack.config.js",
    "builddebug": "cross-env NODE_ENV=production webpack --config etc/webpack.config.js && cross-env NODE_ENV=debug webpack --config etc/webpack.config.js",
    "browserstack": "bash ./etc/runBrowserstackSeq.sh",
    "browserstack:full": "bash ./etc/runFullBrowserstackSeq.sh",
    "pree2e": "node etc/nightwatch.local.conf.js",
    "e2e": "mkdir -p target/reports &&./node_modules/.bin/nightwatch -c etc/nightwatch.local.conf.js -e chrome",
    "e2esafari": "./node_modules/.bin/nightwatch -c etc/nightwatch.local.conf.js -e safari",
    "test": "npm run test:unit && npm run test:node",
    "test:unit": "cross-env NODE_ENV=test karma start etc/karma.conf.ci.js && npm run test:node",
    "test:node": "mocha 3000 test/node",
    "test:selenium": "node etc/nightwatch.local.conf.js && mkdir -p target/reports &&./node_modules/.bin/nightwatch -c etc/nightwatch.jenkins.conf.js -e phantomjs",
    "test:watch": "cross-env NODE_ENV=test karma start etc/karma.conf.js",
    "eslint": "eslint \"src/**/*.js\" && eslint \"etc/*\" && eslint \"test/specs/**/*.js\" && eslint \"test/e2e/**/*.js\"",
    "check": "npm run eslint && nsp check",
    "jsdoc": "jsdoc -c ./jsdoc.json -t ./node_modules/ink-docstrap/template -R README.md ./etc",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.15.5",
    "fresh": "^0.5.2",
    "http-auth": "^3.1.3",
    "js-cookie": "^2.1.4",
    "nouislider": "^10.1.0",
    "serve-index": "^1.9.0",
    "serve-static": "^1.12.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "6.2.5",
    "babel-plugin-es6-promise": "1.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-es2015-object-super": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "compass-mixins": "0.12.10",
    "conventional-changelog-cli": "1.2.0",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "3.1.2",
    "css-loader": "0.25.0",
    "es6-promise": "^4.1.1",
    "eslint": "3.7.1",
    "eslint-loader": "1.5.0",
    "eslint-plugin-es6-recommended": "0.1.2",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jasmine": "2.2.0",
    "eslint-plugin-jasmine-jquery": "1.0.0",
    "exports-loader": "0.6.3",
    "expose-loader": "0.7.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "imports-loader": "0.6.5",
    "include-media": "^1.4.9",
    "istanbul-instrumenter-loader": "1.0.0",
    "jasmine": "2.5.3",
    "jasmine-core": "2.5.2",
    "jasmine-jquery": "2.1.1",
    "jasmine-node": "^1.14.5",
    "jquery": "3.2.1",
    "json-loader": "0.5.4",
    "karma": "1.5.0",
    "karma-babel-preprocessor": "6.0.1",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-es6-shim": "1.0.0",
    "karma-jasmine": "1.0.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-jasmine-jquery": "0.1.1",
    "karma-junit-reporter": "1.2.0",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "mocha": "^3.5.0",
