{
  "name": "oil",
  "version": "0.0.1",
  "description": "Module for Axel Springer Oil",
  "main": "oil",
  "scripts": {
    "start": "npm install && node http_server.js",
    "watch": "cross-env NODE_ENV=development webpack-dev-server --hot --config etc/webpack.config.js --inline --progress --port 3000 ",
    "prebuild": "npm run check && rm -rf dist/*",
    "build": "cross-env NODE_ENV=debug webpack --config etc/webpack.config.js && cross-env NODE_ENV=production webpack --config etc/webpack.config.js",
    "watch-test": "cross-env NODE_ENV=test karma start etc/karma.conf.js",
    "test": "cross-env NODE_ENV=test karma start etc/karma.conf.ci.js",
    "browserstack": "bash ./etc/runBrowserstackSeq.sh",
    "fullBrowserstack": "bash ./etc/runFullBrowserstackSeq.sh",
    "pree2e": "node etc/nightwatch.local.conf.js",
    "e2e": "./node_modules/.bin/nightwatch -c etc/nightwatch.local.conf.js -e chrome",
    "eslint": "eslint \"lib/*\" && eslint \"etc/*\"",
    "check": "npm run eslint && nsp check",
    "jsdoc": "jsdoc -c ./jsdoc.json -t ./node_modules/ink-docstrap/template -R README.md ./etc",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "changelog:add": "git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md'",
    "release:pre": "npm run build && npm run test",
    "release:post": "npm run changelog && npm run changelog:add",
    "release:major": "npm version major && npm run release:pre && git push origin && git push origin --tags && npm run release:post && npm publish",
    "release:minor": "npm version minor && npm run release:pre && git push origin && git push origin --tags && npm run release:post && npm publish",
    "release:patch": "npm version patch && npm run release:pre && git push origin && git push origin --tags && npm run release:post && npm publish",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "compression": "1.6.2",
    "cors": "2.8.3",
    "express": "4.15.2",
    "http-auth": "3.1.3",
    "js-cookie": "2.1.4",
    "serve-static": "1.12.1"
  },
  "devDependencies": {
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-plugin-es6-promise": "1.1.1",
    "babel-plugin-transform-class-properties": "6.16.0",
    "babel-plugin-transform-es2015-arrow-functions": "6.8.0",
    "babel-plugin-transform-es2015-classes": "6.14.0",
    "babel-plugin-transform-es2015-destructuring": "6.16.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.16.0",
    "babel-plugin-transform-es2015-object-super": "6.8.0",
    "babel-preset-es2015": "6.16.0",
    "babel-register": "6.16.3",
    "compass-mixins": "0.12.10",
    "conventional-changelog-cli": "1.2.0",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "3.1.2",
    "css-loader": "0.25.0",
    "eslint": "3.7.1",
    "eslint-loader": "1.5.0",
    "eslint-plugin-es6-recommended": "0.1.2",
    "eslint-plugin-import": "2.0.0",
    "eslint-plugin-jasmine": "2.2.0",
    "eslint-plugin-jasmine-jquery": "1.0.0",
    "exports-loader": "0.6.3",
    "expose-loader": "0.7.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "imports-loader": "0.6.5",
    "include-media": "^1.4.9",
    "istanbul-instrumenter-loader": "1.0.0",
    "jasmine": "2.5.3",
    "jasmine-core": "2.5.2",
    "jasmine-jquery": "2.1.1",
    "jquery": "3.2.1",
    "json-loader": "0.5.4",
    "karma": "1.5.0",
    "karma-babel-preprocessor": "6.0.1",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-es6-shim": "1.0.0",
    "karma-jasmine": "1.0.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-jasmine-jquery": "0.1.1",
    "karma-junit-reporter": "1.2.0",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "nightwatch": "0.9.14",
    "node-sass": "3.10.1",
    "nsp": "2.6.2",
    "open-browser-webpack-plugin": "0.0.2",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "2.1.13",
    "raw-loader": "0.5.1",
    "remap-istanbul": "0.6.4",
    "sass-loader": "4.0.2",
    "selenium-download": "2.0.9",
    "source-map-loader": "0.1.5",
    "style-loader": "0.13.1",
    "to-string-loader": "1.1.5",
    "webpack": "2.3.3",
    "webpack-dev-server": "2.4.2",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "0.14.1",
    "webpack-visualizer-plugin": "0.1.5"
  },
  "peerDependencies": {},
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/as-ideas/oil.git"
  },
  "author": "Martin Reinhardt",
  "license": "UNLICENSED",
  "engine-strict": true,
  "engines": {
    "node": ">= 4.4",
    "npm": ">= 3"
  }
}
